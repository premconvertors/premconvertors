                        <a href="{{ post.url }}" aria-label="Read more about the blog, {{ post.data.title }}">
                            <img src="{{ post.data.image }}" class="img-fluid news-image" alt="{{ post.data.imageAlt }}" loading="lazy">
                        </a>
                        
                        <div class="news-text-info news-text-info-large">
                            <span class="category-tag bg-danger">Featured</span>

                            <h5 class="news-title mt-2">
